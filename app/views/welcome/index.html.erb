<h1> Welcome to Quora </h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>
  <tr>
    <th>Tag</th>
    <th>Content</th>
    <th>By</th>

  </tr>

  <tbody>
    <% @questions.each do |question| %>
      <tr>
        <td><%= question.tag %></td>
        <td><%= question.content %></td>
        <td><%= question.user.email %></td>

      </tr>
    <% end %>
    </tbody>
    </table>

    <br>

    <%= link_to 'New Question', new_question_path %>
